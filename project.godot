config_version=5

[application]

config/name="knakk"
run/main_scene="res://Main.tscn"
config/features=PackedStringArray("4.3", "Mobile")
run/max_fps=60
boot_splash/bg_color=Color(0.996078, 0.996078, 0.996078, 1)
boot_splash/show_image=false
config/icon="res://Branding/Icon.png"
